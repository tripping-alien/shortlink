msgstr "1 час"

msgid "24 Hours"
msgstr "24 часа"

msgid "1 Week"
msgstr "1 неделя"

msgid "Never"
msgstr "Никогда"

# Result box heading
msgid "Your short link:"
msgstr "Ваша короткая ссылка:"

# Copy button text
msgid "Copy"
msgstr "Копировать"

# Copied button text
msgid "Copied!"
msgstr "Скопировано!"

# Footer
